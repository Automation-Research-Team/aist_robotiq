<?xml version="1.0"?>
<package format="2">

  <name>aist_robotiq2f85_controller</name>
  <version>0.1.0</version>
  <description> aist_robotiq2f85_controller package</description>
  <license>TODO</license>

  <maintainer email="j.miyoshi@aist.go.jp">Jun Miyoshi</maintainer>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>angles</build_depend>

  <depend>control_toolbox</depend>
  <depend>robot_controllers</depend>
  <depend>robot_controllers_interface</depend>
  <depend>robotiq_msgs</depend>
  <depend>ur_modern_driver</depend>

  <exec_depend>actionlib</exec_depend>
  <exec_depend>control_msgs</exec_depend>
  <exec_depend>robotiq_control</exec_depend>
  <exec_depend>robotiq_description</exec_depend>
  <exec_depend>trajectory_msgs</exec_depend>

  <export>
    <robot_controllers plugin="${prefix}/robotiq_controllers.xml"/>
  </export>

</package>
