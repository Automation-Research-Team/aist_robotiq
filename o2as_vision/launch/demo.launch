<launch>
	<include file="$(find o2as_moveit_config)/launch/demo.launch">
	</include>

    <!-- camera1 -->
    <arg name="camera1" default="b_bot_camera"/>
    <arg name="camera1_type" default="realsense"/>
    <arg name="camera1_serial_number" default="616205004841"/>
    <!-- <arg name="camera1_serial_number" default="612201001671"/> -->
    <!-- <arg name="camera1_serial_number" default="616205004841"/> -->
	<include file="$(find o2as_vision)/launch/vision_group_realsense.launch">
        <arg name="camera" value="$(arg camera1)"/>
        <arg name="camera_type" value="$(arg camera1_type)"/>
        <arg name="camera_serial_number" value="$(arg camera1_serial_number)"/>
    </include>

    <!-- camera1 -->
    <arg name="camera_list" default="[$(arg camera1)]"/>
    <arg name="parts_list_filename" default="$(find o2as_parts_description)/config/assembly_parts_list.yaml"/>
	<include file="$(find o2as_vision)/launch/vision.launch">
	    <arg name="parts_list_filename" value="$(arg parts_list_filename)"/>
	    <arg name="camera_list" value="$(arg camera_list)"/>
    </include>
    
    <!-- 
    -1 : all 
     0 : holder (for test)
     7 : bearing housing
    11 : output pulley
    13 : idler pulley
    -->
    <arg name="object_id" default="0"/>
    <node name="o2as_vision_demo" pkg="o2as_vision" type="realsense_camera_demo.py" output="screen">
	    <param name="object_id" type="string" value="$(arg object_id)"/>
    </node>
</launch>
