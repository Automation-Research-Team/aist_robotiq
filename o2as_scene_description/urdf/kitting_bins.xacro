  </xacro:kitting_bin_3>
  <!--set2_bin1_1_-->
  <xacro:kitting_bin_1 binname="set2_bin1_1" parent="workspace_center" z_origin_offset="-.008">
      <origin xyz=".449 0.1825 0.023" rpy="0 0 0" />
  </xacro:kitting_bin_1>
  <!--set2_bin1_2_-->
