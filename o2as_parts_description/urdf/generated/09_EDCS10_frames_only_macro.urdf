<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="assy_part_frames_only_09">
  <xacro:macro name="assy_part_frames_only_09" params="prefix parent spawn_attached *origin">
    <link name="${prefix}assy_part_09"/>

    <xacro:if value="${spawn_attached}">
      <joint name="${prefix}assy_part_09_joint" type="fixed">
        <parent link="${parent}"/>
        <child link="${prefix}assy_part_09"/>
        <xacro:insert_block name="origin"/>
      </joint>
    </xacro:if>
  
  <!-- Extra frames for assembly and motion planning, if any -->
    <joint name="${prefix}assy_part_09_inner_surface_joint" type="fixed"> 
      <parent link="${prefix}assy_part_09"/> 
      <child link="${prefix}assy_part_09_inner_surface"/> 
      <origin rpy="${0} ${0} ${0}" xyz="${0.002} ${0} ${0}"/> 
    </joint> 
    <link name="${prefix}assy_part_09_inner_surface"/> 
    
    <joint name="${prefix}assy_part_09_screw_hole_joint" type="fixed"> 
      <parent link="${prefix}assy_part_09"/> 
      <child link="${prefix}assy_part_09_screw_hole"/> 
      <origin rpy="${0} ${0} ${pi}" xyz="${0} ${0} ${0}"/> 
    </joint> 
    <link name="${prefix}assy_part_09_screw_hole"/> 
    

  </xacro:macro>
</robot>
