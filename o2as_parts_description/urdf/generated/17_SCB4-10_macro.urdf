<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="assy_part_17">
  <xacro:macro name="assy_part_17" params="prefix parent spawn_attached *origin">
    <link name="${prefix}assy_part_17">
      <visual>
        <origin rpy="1.5707 0 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://o2as_parts_description/meshes/17_SCB4-10.dae" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="white">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
      <collision>
        <origin rpy="1.5707 0 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://o2as_parts_description/meshes/17_SCB4-10.vhacd.dae" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
    </link>

    <xacro:if value="${spawn_attached}">
      <joint name="${prefix}assy_part_17_joint" type="fixed">
        <parent link="${parent}"/>
        <child link="${prefix}assy_part_17"/>
        <xacro:insert_block name="origin"/>
      </joint>
    </xacro:if>
  
  <!-- Extra frames for assembly and motion planning, if any -->
    <joint name="${prefix}assy_part_17_screw_tip_joint" type="fixed"> 
      <parent link="${prefix}assy_part_17"/> 
      <child link="${prefix}assy_part_17_screw_tip"/> 
      <origin rpy="${0} ${0} ${0}" xyz="${0.010} ${0} ${0}"/> 
    </joint> 
    <link name="${prefix}assy_part_17_screw_tip"/> 
    
    <joint name="${prefix}assy_part_17_screw_inserted_joint" type="fixed"> 
      <parent link="${prefix}assy_part_17"/> 
      <child link="${prefix}assy_part_17_screw_inserted"/> 
      <origin rpy="${0} ${0} ${0}" xyz="${0} ${0} ${0}"/> 
    </joint> 
    <link name="${prefix}assy_part_17_screw_inserted"/> 
    
    <joint name="${prefix}assy_part_17_screw_head_joint" type="fixed"> 
      <parent link="${prefix}assy_part_17"/> 
      <child link="${prefix}assy_part_17_screw_head"/> 
      <origin rpy="${0} ${0} ${0}" xyz="${-0.004} ${0} ${0}"/> 
    </joint> 
    <link name="${prefix}assy_part_17_screw_head"/> 
    

  </xacro:macro>
</robot>
