<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="assy_part_14">
  <xacro:macro name="assy_part_14" params="prefix parent spawn_attached *origin">
    <link name="${prefix}assy_part_14">
      <visual>
        <origin rpy="1.5707 0 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://o2as_parts_description/meshes/14_BGPSL6-9-L30-F8.dae" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="white">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
      <collision>
        <origin rpy="1.5707 0 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://o2as_parts_description/meshes/14_BGPSL6-9-L30-F8.vhacd.dae" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
    </link>

    <xacro:if value="${spawn_attached}">
      <joint name="${prefix}assy_part_14_joint" type="fixed">
        <parent link="${parent}"/>
        <child link="${prefix}assy_part_14"/>
        <xacro:insert_block name="origin"/>
      </joint>
    </xacro:if>
  
  <!-- Extra frames for assembly and motion planning, if any -->
    <joint name="${prefix}assy_part_14_screw_tip_joint" type="fixed"> 
      <parent link="${prefix}assy_part_14"/> 
      <child link="${prefix}assy_part_14_screw_tip"/> 
      <origin rpy="${0} ${0} ${0}" xyz="${0.03} ${0} ${0}"/> 
    </joint> 
    <link name="${prefix}assy_part_14_screw_tip"/> 
    
    <joint name="${prefix}assy_part_14_screw_inserted_joint" type="fixed"> 
      <parent link="${prefix}assy_part_14"/> 
      <child link="${prefix}assy_part_14_screw_inserted"/> 
      <origin rpy="${0} ${0} ${0}" xyz="${0} ${0} ${0}"/> 
    </joint> 
    <link name="${prefix}assy_part_14_screw_inserted"/> 
    
    <joint name="${prefix}assy_part_14_screw_head_joint" type="fixed"> 
      <parent link="${prefix}assy_part_14"/> 
      <child link="${prefix}assy_part_14_screw_head"/> 
      <origin rpy="${0} ${0} ${0}" xyz="${-0.002} ${0} ${0}"/> 
    </joint> 
    <link name="${prefix}assy_part_14_screw_head"/> 
    

  </xacro:macro>
</robot>
