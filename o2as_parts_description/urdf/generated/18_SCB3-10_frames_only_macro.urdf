<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="assy_part_frames_only_18">
  <xacro:macro name="assy_part_frames_only_18" params="prefix parent spawn_attached *origin">
    <link name="${prefix}assy_part_18"/>

    <xacro:if value="${spawn_attached}">
      <joint name="${prefix}assy_part_18_joint" type="fixed">
        <parent link="${parent}"/>
        <child link="${prefix}assy_part_18"/>
        <xacro:insert_block name="origin"/>
      </joint>
    </xacro:if>
  
  <!-- Extra frames for assembly and motion planning, if any -->
    <joint name="${prefix}assy_part_18_screw_tip_joint" type="fixed"> 
      <parent link="${prefix}assy_part_18"/> 
      <child link="${prefix}assy_part_18_screw_tip"/> 
      <origin rpy="${0} ${0} ${0}" xyz="${0.010} ${0} ${0}"/> 
    </joint> 
    <link name="${prefix}assy_part_18_screw_tip"/> 
    
    <joint name="${prefix}assy_part_18_screw_inserted_joint" type="fixed"> 
      <parent link="${prefix}assy_part_18"/> 
      <child link="${prefix}assy_part_18_screw_inserted"/> 
      <origin rpy="${0} ${0} ${0}" xyz="${0} ${0} ${0}"/> 
    </joint> 
    <link name="${prefix}assy_part_18_screw_inserted"/> 
    
    <joint name="${prefix}assy_part_18_screw_head_joint" type="fixed"> 
      <parent link="${prefix}assy_part_18"/> 
      <child link="${prefix}assy_part_18_screw_head"/> 
      <origin rpy="${0} ${0} ${0}" xyz="${-0.003} ${0} ${0}"/> 
    </joint> 
    <link name="${prefix}assy_part_18_screw_head"/> 
    

  </xacro:macro>
</robot>
