<launch>

  <arg name="sim"		default="false"/>
  <arg name="rviz_config"	default=""/>
  <arg name="debug"		default="false"/>

  <param name="scene_description"
  	 command="$(find xacro)/xacro --inorder '$(find
  		  aist_scene_description)/urdf/ceatec_base_scene.urdf.xacro'"/>

  <node name="map_world" pkg="tf" type="static_transform_publisher"
        args="0.3 0.5 0 0 0 0 map world 10"/>

  <include file="$(find aist_routines)/launch/moveit_planning_execution.launch">
    <arg name="moveit_config"	value="$(find ceatec_moveit_config)"/>
    <arg name="rviz_config"	value="$(arg rviz_config)"/>
    <arg name="debug"		value="$(arg debug)"/>
  </include>

</launch>
