<launch>
  <arg name="sim" default="false" />

  <include file="$(find aist_examples)/launch/connect_aist_robots.launch" unless="$(arg sim)" />
  <include file="$(find aist_moveit_config)/launch/aist_moveit_planning_execution.launch">
    <param name="sim" value="$(arg sim)" />
  </include>

  <include file="$(find o2as_phoxi_camera)/launch/o2as_phoxi_camera.launch" />
  <include file="$(find aist_skills)/launch/aist_skill_server.launch" />

  <rosparam command="delete" param="part_bin_list" />
  <rosparam command="load" file="$(find aist_routines)/config/part_bin_list.yaml" />

</launch>
